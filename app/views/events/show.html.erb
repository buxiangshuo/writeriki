<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @event.name %>
</p>

<p>
  <b>Content:</b>
  <%= @event.content %>
</p>

<p>
  <b>Level:</b>
  <%= @event.level %>
</p>

<p>
  <b>Appointment:</b>
  <%= @event.appointment %>
</p>

<p>
  <b>Start at:</b>
  <%= @event.start_at.strftime("%Y-%m-%d") %>
</p>

<p>
  <b>End at:</b>
  <%= @event.end_at.strftime("%Y-%m-%d") %>
</p>

<p>
  <b>User:</b>
  <%= @event.user_id %>
</p>


<%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'Back', events_path %>
